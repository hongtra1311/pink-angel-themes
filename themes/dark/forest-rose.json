    "notificationLink.foreground": "#6D9F71",
    "terminal.background": "#222B2E",
    "terminal.foreground": "#F0F0F0",
    "terminal.ansiBlack": "#222B2E",
    "terminal.ansiRed": "#C24D2C",
    "terminal.ansiGreen": "#6D9F71",
    "terminal.ansiYellow": "#EA9AB2",
    "terminal.ansiBlue": "#4A7D9A",
    "terminal.ansiMagenta": "#E27396",
    "terminal.ansiCyan": "#337357",
    "terminal.ansiWhite": "#F0F0F0",
    "terminal.ansiBrightBlack": "#9AAAB2",
    "terminal.ansiBrightRed": "#A43820",
    "terminal.ansiBrightGreen": "#337357",
    "terminal.ansiBrightYellow": "#E27396",
    "terminal.ansiBrightBlue": "#4A7D9A",
    "terminal.ansiBrightMagenta": "#EA9AB2",
    "terminal.ansiBrightCyan": "#6D9F71",
    "terminal.ansiBrightWhite": "#F0F0F0"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#337357",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage", "modifier"],
      "settings": {
        "foreground": "#E27396"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable",
        "support.variable",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#6D9F71"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "constant.other.symbol",
        "constant.other.enum",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#EA9AB2"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#6D9F71"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "meta.tag.construct",
        "meta.tag.metadata",
        "meta.tag.xml",
        "meta.selector.css entity.name.tag"
      ],
      "settings": {
        "foreground": "#E27396"
      }
    },
    {
      "scope": ["punctuation", "delimiter", "bracket"],
      "settings": {
        "foreground": "#F0F0F0"
      }
    },
    {
      "scope": ["operator"],
      "settings": {
        "foreground": "#337357"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.boolean"],
      "settings": {
        "foreground": "#4A7D9A"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#EA9AB2"
      }
    },
    {
      "scope": ["support.type", "storage.type"],
      "settings": {
        "foreground": "#EA9AB2"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#C24D2C"
      }
    },
    {
      "scope": ["invalid.illegal"],
      "settings": {
        "foreground": "#A43820"
      }
    }
  ]
}
