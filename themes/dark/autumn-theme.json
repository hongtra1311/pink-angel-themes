{
  "name": "LGBT Spectrum - Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#FFFFFF",
    "activityBar.background": "#181818",
    "sideBar.background": "#1C1C1C",
    "sideBar.foreground": "#E0E0E0",
    "statusBar.background": "#1B1B1B",
    "titleBar.activeBackground": "#1B1B1B",
    "tab.activeBackground": "#242424",
    "tab.inactiveBackground": "#1C1C1C",
    "tab.activeForeground": "#FFFFFF",
    "tab.inactiveForeground": "#A6A6A6",
    "editorLineNumber.foreground": "#666666",
    "editorCursor.foreground": "#FFD700",
    "editor.selectionBackground": "#333333",
    "editor.wordHighlightBackground": "#2A2A2A",
    "editor.lineHighlightBackground": "#252525",
    "editorWhitespace.foreground": "#3B3B3B",
    "editorIndentGuide.activeBackground": "#4B4B4B",
    "editorIndentGuide.background": "#323232",
    "button.background": "#333333",
    "button.hoverBackground": "#444444",
    "panel.background": "#1E1E1E",
    "terminal.background": "#1B1B1B",
    "terminal.foreground": "#FFFFFF",
    "activityBar.foreground": "#FFFFFF",
    "activityBarBadge.background": "#FF69B4",
    "statusBar.foreground": "#FFFFFF"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": { "foreground": "#888888" }
    },
    {
      "scope": ["keyword"],
      "settings": { "foreground": "#FF5555" }
    },
    {
      "scope": ["string"],
      "settings": { "foreground": "#FFB86C" }
    },
    {
      "scope": ["number", "constant"],
      "settings": { "foreground": "#FFD700" }
    },
    {
      "scope": ["variable", "parameter"],
      "settings": { "foreground": "#50FA7B" }
    },
    {
      "scope": ["function", "method"],
      "settings": { "foreground": "#66CCFF" }
    },
    {
      "scope": ["type", "class", "interface"],
      "settings": { "foreground": "#9370DB" }
    },
    {
      "scope": ["decorator", "attribute", "tag"],
      "settings": { "foreground": "#FF69B4" }
    },
    {
      "scope": ["operator", "punctuation"],
      "settings": { "foreground": "#CCCCCC" }
    }
  ]
}
